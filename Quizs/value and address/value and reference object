class  Quiz
/**
@author wq
*/
{
	public static void main(String[] args) 
	{
		String A="I'm old one";
		String B;
		B=A;
		B="I'm new one";
		System.out.println(A);//String是基本数据类型,不是引用类型

		System.out.println(A.hashCode());//将A的存储地址、字段等,映射成一个数值(散列值)

		String[]C={"abcd"};
		System.out.println(C);//JNI字段描述符
	}
}
